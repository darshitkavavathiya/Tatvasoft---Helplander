<!doctype html>

<html lang="en">

<head>


	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">


	<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
		  integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
	<link href='https://fonts.googleapis.com/css?family=Roboto' rel='stylesheet'>

  
	<link rel="stylesheet" href="~/css/faq.css" />
	<link rel="stylesheet" href="~/css/navbar.css" />
	@RenderSection("Css",required:false)


	<title>@ViewData["Title"]</title>

</head>


	






<body>


	<!-- header section -->
	<!-- nav bar -->

	   @await Html.PartialAsync("navbar")


	@RenderBody()


	<!--footer-->

	<section class="our-news-letter">
		<div class="container text-center">
			<h2>GET OUR NEWSLETTER</h2>
			<div class="form-row d-flex justify-content-center align-items-center">
				<div class="form-group">
					<label for="email" style="display: none;">YOUR EMAIL</label>
					<input type="text" placeholder="YOUR EMAIL" id="email" class="form-control">
				</div>
				<div class="btn-wrapper">
					<button class="submit">Submit</button>
				</div>
			</div>
		</div>
	</section>

	<div class=" footer-container">

		<div class="row">

			<div class="col-sm-3 footer-logo-col">
				<img src="~/Images/footer-logo.png" class="img-fluid footer-logo-image">
			</div>


			<div class="col-sm-6 footer-nav-col">

				<a class="footer-link " asp-controller="Public" asp-action="Index"> &nbsp;HOME</a>
				<a class="footer-link " asp-controller="Public" asp-action="About"> ABOUT</a>
				<a class="footer-link "> &nbsp;TESTIMONIALS</a>
				<a class="footer-link " asp-controller="Public" asp-action="Faq"> &nbsp;FAQ</a>
				<a class="footer-link "> &nbsp;INSURANCE</a>
				<a class="footer-link "> &nbsp;IMPRESSUM</a>

			</div>

			<div class="col-sm-3 footer-social-col">
				<img src="~/Images/ic-facebook.png" class="img-fluid footer-social-image">

				<img src="~/Images/ic-instagram.png" class="img-fluid footer-social-image">
			</div>


		</div>
		<div class="row footer-term-row">
			<hr class="fluid footer-hr">
			<p class="footer-text">
				©2018 Helperland. All rights reserved.&nbsp;&nbsp; &nbsp;&nbsp; <a>Terms and Conditions </a>|<a>
					&nbsp;Privacy
					Policy
				</a>
			</p>

		</div>

	</div>






	<!-- script -->

	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
			integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
			crossorigin="anonymous"></script>
			 
	

</body>

</html>

